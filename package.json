{
  "name": "jimbo-copilot-extension",
  "displayName": "Jimbo Copilot Extension",
  "description": "A VS Code extension that displays Jimbo head from Balatro with speech bubbles for Copilot changes",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jimboCopilot.testReaction",
        "title": "Test Jimbo Reaction",
        "category": "Jimbo Copilot"
      },
      {
        "command": "jimboCopilot.openPanel",
        "title": "Open Jimbo Panel",
        "category": "Jimbo Copilot"
      },
      {
        "command": "jimboCopilot.reloadQuotes",
        "title": "Reload Quotes from JSON",
        "category": "Jimbo Copilot"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "jimboPanel",
          "title": "Jimbo Copilot",
          "icon": "$(play)"
        }
      ]
    },
    "views": {
      "jimboPanel": [
        {
          "id": "jimboView",
          "name": "Jimbo Copilot",
          "type": "webview",
          "contextualTitle": "Jimbo Copilot"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.18.126",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  }
}
